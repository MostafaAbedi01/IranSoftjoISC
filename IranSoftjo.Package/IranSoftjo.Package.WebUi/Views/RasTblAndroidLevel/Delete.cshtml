@using IranSoftjo.Core.Web.Mvc.Html
@model IranSoftjo.Package.WebUi.ViewModels.TblAndroidLevelVm
@{
    ViewBag.Title = "حذف ";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<hgroup class="title">
    <h1>@ViewBag.Title</h1>
    <hr />
</hgroup>


<div style="box-shadow: 0 1px 2px rgba(0, 0, 0, 0.5); height: auto;">
    <div>

        @Html.EnhancedLabelField(model => model.KeyID1Title)
        @Html.EnhancedLabelField(model => model.KeyID2Title)
        @Html.EnhancedLabelField(model => model.Metraj)
        @Html.EnhancedLabelField(model => model.Hardener)
        @Html.EnhancedLabelField(model => model.Thinner)
        @Html.EnhancedLabelField(model => model.Color)
        @Html.EnhancedLabelField(model => model.Comment)
        @Html.EnhancedLabelField(model => model.FirrstAndLastName)
        @using (Html.BeginForm())
        {
            <table class="form">
                @Html.AntiForgeryToken()
                @Html.HiddenFor(model => model.TblProjectId)
                @Html.EnhancedSubmit("حذف")
            </table>
        }
    </div>

</div>

<div>
    @Html.ActionLink("بازگشت به لیست", "TblAndroidLevelList")
</div>

