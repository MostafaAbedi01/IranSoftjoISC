@using IranSoftjo.Core.Web.Mvc.Html
@model IranSoftjo.Package.WebUi.ViewModels.TblSubItemAccessVm

@{
    ViewBag.Title = "حذف مرحله";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<hgroup class="title">
    <h1>@ViewBag.Title</h1>
    <hr />
</hgroup>


<div>

    @Html.EnhancedLabelField(model => model.KeyID1Title)
    @Html.EnhancedLabelField(model => model.KeyID2Title)

    @Html.EnhancedLabelField(model => model.Title)
    @using (Html.BeginForm())
    {
        <table class="form">
            @Html.AntiForgeryToken()
            @Html.HiddenFor(model => model.TblSubItemAccessId)
            @Html.EnhancedSubmit("حذف ")
        </table>
    }


</div>

<div>
    @Html.ActionLink("بازگشت به لیست", "SubItemAccessList", new { id = Model.TblSubItemId })
</div>